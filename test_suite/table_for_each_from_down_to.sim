#!/usr/bin/env simple

$ages
  key_order_table
    "Stefan" = 18
    "Doris" = 29
    "Herbert" = 26
    "Carla" = 4
    "Kurt" = 21

for_each_from_down_to ages "Kurt" "Doris": (name age)
  writeln name ": " age
writeln "----------"

for_each_from_down_to ages "Doris" undefined: (name age)
  writeln name ": " age
writeln "----------"

for_each_from_down_to ages undefined "Kurt": (name age)
  writeln name ": " age
